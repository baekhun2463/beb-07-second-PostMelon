{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base", "plugin:node/recommended", "prettier"],
  "plugins": ["prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "prettier/prettier": "error",
    "no-unused-vars": "warn",
    "no-console": "off",
    "func-names": "off",
    "class-methods-use-this": "off",
    "no-plusplus": "off",
    "no-param-reassign": "warn",
    "no-underscore-dangle": "warn"
  },
  "globals": {
    "describe": true,
    "it": true,
    "test": true
  }
}
